{
  "name": "react-valid-props",
  "version": "1.0.11",
  "description": "A little helper to get around unknown property in react 15.2",
  "main": "dist/react-valid-props.js",
  "typings": "index.d.ts",
  "files": [
    "src",
    "dist",
    "index.d.ts",
    ".babelrc",
    "README.md"
  ],
  "scripts": {
    "test": "mocha -r babel-core/register src/react-valid-props.spec.js",
    "compile": "babel src --out-dir dist",
    "compile:watch": "babel src --out-dir dist --watch",
    "publish-patch": "npm version patch && git push --tags"
  },
  "keywords": [
    "react",
    "unknown-prop",
    "15.2"
  ],
  "author": "Jonathan Glanz",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "mocha": "^2.5.3"
  }
}
